<form method="post" action="{{ path('app_travel_plan_delete', {'id': travel_plan.id}) }}" onsubmit="return confirm('Are you sure you want to delete this plan?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ travel_plan.id) }}">
	<button class="btn btn-d btn-animated">Delete</button>
</form>
